                    <f:content>
                        <core:Title id="_IDGenTitle1" />
                        <Label id="_IDGenLabel1" text="{i18n>lotNo}" />
                        <Input id="_IDGenInput1" value="{SupplierName}" change="onLotPress"/>
                        <core:Title id="_IDGenTitle2" />
                        <Label id="_IDGenLabel2" text="{i18n>lotIssueDate}" />
                        <DatePicker id="_IDGenDatePicker1" valueFormat="yyyy-MM-dd" displayFormat="yyyy-MM-dd" enabled="false"/>
                        <core:Title id="_IDGenTitle3"/>
                        <Label id="_IDGenLabel3" text="{i18n>material}" />
                        <Input id="_IDGenInput2" value="{SupplierName}" valueHelpOnly="true" valueHelpRequest="onLotPress" showValueHelp="true"/>
