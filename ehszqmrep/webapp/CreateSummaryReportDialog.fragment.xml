<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog id="create-smr-rep-dialog" title="{i18n>createSummaryRepDialogTitle}">
		<content>
			<List id="create-smr-rep-dialog-form-type-list" mode="SingleSelectMaster" selectionChange="onSelectFormDialog"
				items="{path: '/I_IncidentReportForms'}">
				<items>
					<StandardListItem id="formTypeListItem" title="{EHSFormName_Text}"/>
				</items>
			</List>
		</content>
				<endButton>
			<Button id="cancelSelectFormBtn" text="{i18n>cancelBtn}" press="onPressCancelSelectFormDialog"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>